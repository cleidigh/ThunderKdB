{
	"manifest_version": 2,
	"applications": {
		"gecko": {
			"id": "signal-spam@signal-spam.fr",
			"strict_min_version": "77.0"
		}
	},
	"name": "Signal Spam",
	"author": "VERIFROM for Signal Spam",
	"description": "__MSG_description__",
	"version": "4.0.5",
	"icons": {
		"16": "logo/16x16.png",
		"32": "logo/32x32.png",
		"48": "logo/48x48.png",
		"96": "logo/96x96.png",
		"128": "logo/128x128.png",
		"256": "logo/256x256.png"
	},
	"default_locale": "en",
	"options_ui": {
		"page": "html/options.html",
		"open_in_tab": true
	},
	"permissions": [
		"notifications",
		"messagesRead",
		"storage",
		"activeTab",
		"tabs",
		"menus",
		"accountsRead",
		"messagesMove",
		"https://*.signal-spam.fr/*"
	],
	"experiment_apis": {
		"notification": {
			"schema": "experiments/notification/schema.json",
			"parent": {
				"scopes": [
					"addon_parent"
				],
				"paths": [
					[
						"notification"
					]
				],
				"script": "experiments/notification/notification.js"
			}
		},
		"legacyprefs": {
			"schema": "experiments/legacyprefs/schema.json",
			"parent": {
				"scopes": [
					"addon_parent"
				],
				"paths": [
					[
						"legacyprefs"
					]
				],
				"script": "experiments/legacyprefs/legacyprefs.js"
			}
		}
	},
	"content_security_policy": "script-src 'self' 'sha256-Le3UjCT7ZQcJphS9E7Z53Jlvx8+rC2zKNv2shRMlptk='; object-src 'self'; ",
	"background": {
		"scripts": [
			"js/purify.min.js",
			"js/xxhash.js",
			"js/uint64.js",
			"js/xxhash64.js",
			"js/URLCanonicalization.js",
			"js/socket.io.js",
			"specific/extensionConfig.js",
			"js/verifrom_api.js",
			"js/verifrom-localize.js",
			"background.js"
		]
	},
	"browser_action": {
		"browser_style": false,
		"default_icon": "logo/logosignalspam.svg",
		"default_title": "__MSG_report__",
		"theme_icons": [
			{
				"light": "logo/light-logosignalspam.svg",
				"dark": "logo/logosignalspam.svg",
				"size": 16
			},
			{
				"light": "logo/light-logosignalspam.svg",
				"dark": "logo/logosignalspam.svg",
				"size": 32
			}
		]
	},
	"message_display_action": {
		"browser_style": false,
		"default_title": "__MSG_report__",
		"default_icon": "logo/logosignalspam.svg",
		"theme_icons": [
			{
				"light": "logo/light-logosignalspam.svg",
				"dark": "logo/logosignalspam.svg",
				"size": 16
			},
			{
				"light": "logo/light-logosignalspam.svg",
				"dark": "logo/logosignalspam.svg",
				"size": 32
			}
		]
	},
	"web_accessible_resources": []
}