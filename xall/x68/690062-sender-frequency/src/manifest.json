{
  "manifest_version": 2,
  "name": "Sender Frequency",
  "description": "Adds a custom column for Sender Frequency to the thread pane",
  "version": "2.0.3",
  "author": "Jorg K",
  "applications": {
    "gecko": {
      "id": "jorgk@sender-frequency",
      "strict_min_version": "68.0a1"
    }
  },
  "experiment_apis": {
    "SFreq": {
      "schema": "schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          ["SFreq"]
        ],
        "script": "experiments.js",
        "events": ["startup"]
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
