{
  "manifest_version": 2,
  "name": "G Suite in tab",
  "description": "Open G Suite apps in new tab",
  "version": "0.3.0",
	"developer": {
		"name": "Lucas Bonomo"
	},
  "homepage_url": "https://lucasbonomo.com/thunderbird",
  "applications": {
    "gecko": {
      "id": "gsuiteintab@lucasbonomo.com",
      "strict_min_version": "78.0"
    }
  },
  "browser_action": {
      "default_popup": "gsuite.html",
      "default_title": "G Suite",
      "default_icon": "icons/gsuite_128.png"
    },
  "permissions": [
  ],
  "icons": {
    "32": "icons/gsuite_32.png",
    "64": "icons/gsuite_64.png",
    "128": "icons/gsuite_128.png",
    "256": "icons/gsuite_256.png"
  }
}
