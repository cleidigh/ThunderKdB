{
  "browser_specific_settings": {
    "gecko": {
      "id": "opengooglecalendar@glicer",
      "strict_min_version": "68.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Open Google Calendar",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "developer": {
    "name": "Emmanuel ROECKER et Rym BOUCHAGOUR",
    "url": "https://www.rymetemmanuel.fr/"
  },  
  "description": "Open up Google Calendar in a new tab",
  "manifest_version": 2,
  "name": "Open Google Calendar",
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies"
  ],
  "applications": {
    "gecko": {
        "id": "opengooglecalendar@glicer"
    }
  },
  "version": "1.6"
}
