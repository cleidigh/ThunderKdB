{
	"manifest_version": 2,
             "applications": {
                "gecko": {
                   "id": "FrameWhite@Sungho.Hwang",
                   "strict_min_version": "78.0"
                 }
             },
	"name": "FrameWhite",
	"version": "62.3",
	"description": "This is a theme with white frames.",
             "icons": {
                "32": "icons/w_large.png"
                },
"browser_action": {
    "default_icon": "icons/w_large.png",
    "default_title": "FrameWhite",
    "default_popup": "popup.html"
  },
"permissions": [
        ],
 "experiment_apis": {
    "setStyle": {
      "schema": "schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "setStyle"
          ]
        ],
        "script": "implementation.js"
      }
    }
  }
}