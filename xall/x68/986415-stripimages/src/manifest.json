{
  "author": "mital",
  "manifest_version": 2,
  "name": "stripImages",
  "description": "Remove all images from message with one click",
  "version": "1.0.9",
  "developer": {
    "name": "mital",
    "url": "https://dotnector.dk"
  },
  "permissions": [
    "compose",
	"menus"
  ],
  "background": {
    "scripts": [
      "content/compose.js"
    ]
  },
  "compose_action": {
    "default_title": "Strip images",
    "default_icon": "content/camera-light.svg"
  },
  "applications": {
    "gecko": {
      "id": "stripImages@mi.tal",
      "strict_min_version": "68.0"
    }
  }
}


